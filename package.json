{
  "name": "ts_deno",
  "version": "1.0.0",
  "description": "deno practice",
  "main": "index.ts",
  "scripts": {
    "start": "deno run --node-modules-dir --reload t--allow-env --allow-read --allow-net src/app.ts",
    "lint": "deno lint",
    "format": "deno fmt",
    "test": "deno test",
    "test:parallel": "deno test --parallel",
    "test:coverage": "deno test --coverage=cov_profile",
    "test:file": "deno test test/example.test.ts",
    "test:doc": "deno test --doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magocod/ts_deno.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magocod/ts_deno/issues"
  },
  "homepage": "https://github.com/magocod/ts_deno#readme",
  "devDependencies": {
    "@types/express": "^4.17.13"
  }
}
